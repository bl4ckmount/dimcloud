{
    "browser_specific_settings": {
      "gecko": {
        "id": "dimcloud@example.com",
        "strict_min_version": "2.0"
      }
    },

    "browser_action": {
      "default_icon": {
        "19": "dimcloud.png",
        "38": "dimcloud.png"
      },
      "default_title": "dimcloud"
    },
    "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
  
    "content_scripts": [
      {
        "matches": ["https://soundcloud.com/*"],
        "js": ["main.js"]
      }
    ],
  
    "description": "Soundcloud dark theme written with dark soul :3",
  
    "icons": {
        "48": "dimcloud.png",
        "96": "dimcloud.png"
    },
  
    "manifest_version": 2,
  
    "name": "dimcloud",
  
    "permissions": ["webNavigation"],
  
    "version": "0.1.3",
  
    "web_accessible_resources": ["main.css"]
}
